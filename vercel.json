{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/optimize.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/signin.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/signup.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/optimize",
      "dest": "/api/optimize.js"
    },
    {
      "src": "/api/signin",
      "dest": "/api/signin.js"
    },
    {
      "src": "/api/signup",
      "dest": "/api/signup.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
