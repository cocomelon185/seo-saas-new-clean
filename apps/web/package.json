{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_OPTIONS=--no-warnings npm run images:opt && NODE_OPTIONS=--no-warnings TAILWIND_CONFIG=./tailwind.marketing.config.cjs vite-react-ssg build && NODE_OPTIONS=--no-warnings TAILWIND_CONFIG=./tailwind.app.config.cjs vite-react-ssg build --mode app-public && NODE_OPTIONS=--no-warnings TAILWIND_CONFIG=./tailwind.app.config.cjs vite build --mode app",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port $PORT",
    "guard:jsx": "node scripts/guard-jsx-parse.mjs",
    "deploy:guard": "npm run guard:jsx && node scripts/predeploy-verify.mjs",
    "postbuild": "node postbuild-seo.mjs",
    "verify:p0": "node scripts/verify-p0.mjs",
    "images:opt": "node scripts/optimize-images.mjs",
    "images:capture": "node scripts/capture-screenshots.mjs",
    "validate:links": "node scripts/validate-links.mjs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@mantine/charts": "^8.3.15",
    "@mantine/core": "^8.3.15",
    "@mantine/hooks": "^8.3.15",
    "@popperjs/core": "^2.11.8",
    "@sentry/react": "^10.39.0",
    "@tabler/icons-react": "^3.37.1",
    "apexcharts": "^5.3.6",
    "beasties": "^0.1.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "critical": "^7.2.1",
    "critters": "^0.0.24",
    "gulp": "^5.0.1",
    "gulp-append-prepend": "^1.0.9",
    "react": "^18.3.1",
    "react-apexcharts": "^1.9.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "resend": "^6.9.2",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "vite": "^7.2.4",
    "vite-react-ssg": "^0.8.9"
  }
}
